<%= form_with(model: job, local: true) do |form| %>
  <% if job.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(job.errors.count, "error") %> prohibited this job from being saved:</h2>

      <ul>
      <% job.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= form.label :type %>
    <%= form.text_field :type %>
  </div>

  <div class="field">
    <%= form.label :label %>
    <%= form.text_field :label %>
  </div>

  <div class="field">
    <%= form.label :user_id %>
    <%= form.text_field :user_id %>
  </div>

  <div class="field">
    <%= form.label :status %>
    <%= form.text_field :status %>
  </div>

  <div class="field">
    <%= form.label :completed_at %>
    <%= form.datetime_select :completed_at %>
  </div>

  <div class="field">
    <%= form.label :collections %>
    <%= form.number_field :collections %>
  </div>

  <div class="field">
    <%= form.label :works %>
    <%= form.number_field :works %>
  </div>

  <div class="field">
    <%= form.label :files %>
    <%= form.number_field :files %>
  </div>

  <div class="actions">
    <%= form.submit %>
  </div>
<% end %>
